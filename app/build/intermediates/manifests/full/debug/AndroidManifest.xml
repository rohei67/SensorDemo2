<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.sensor2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.example.android.sensor2.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.sensor2.CheckerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.android.sensor2.SelectorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.android.sensor2.AccelerometerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.android.sensor2.GravityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.android.sensor2.GyroscopeActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>